<?xml version='1.0' encoding='utf-8'?>
<flashtool-config version="2.0">
    <general>
        <chip-name>MT6572</chip-name>
        <storage-type>EMMC</storage-type>
        <download-agent>MTK_AllInOne_DA.bin</download-agent>
        <scatter>MT6572_Android_scatter.txt</scatter>
        <authentication />
        <certification />
        <rom-list>
            <rom index="0" enable="true">preloader_g368_nyx.bin</rom>
            <rom index="1" enable="true">MBR</rom>
            <rom index="2" enable="true">EBR1</rom>
            <rom index="8" enable="true">lk.bin</rom>
            <rom index="9" enable="true">boot.img</rom>
            <rom index="10" enable="true">recovery.img</rom>
            <rom index="11" enable="true">secro.img</rom>
            <rom index="13" enable="true">logo.bin</rom>
            <rom index="15" enable="true">system.img</rom>
            <rom index="16" enable="true">cache.img</rom>
            <rom index="17" enable="true">userdata.img</rom>
        </rom-list>
        <connection type="BromUSB" high-speed="true" power="AutoDetect" da_log_level="Info" da_log_channel="UART" timeout-count="3600000" com-port="" />
        <checksum-level>none</checksum-level>
        
        <log-info log_on="true" log_path="C:\ProgramData\SP_FT_Logs" clean_hours="720" />
    </general>
    <commands>
        <format-download>
            <combo-format>
                <format validation="false" physical="true" erase-flag="NormalErase" auto-format="true" auto-format-flag="FormatAll" />
            </combo-format>
            <da-download-all />
        </format-download>
    </commands>
</flashtool-config>
